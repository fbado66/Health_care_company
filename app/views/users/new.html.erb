<h1>This is the new for user</h1>

<%= form_for @user do |f| %>
 <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br/>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>
  <%= f.label :email %>
  <%= f.text_field :email %><br/>
  <%= f.label :role %>
  <%= f.select :role, options_for_select(["aide", "client"]) %><br/>
  <%= f.label :password %>
  <%= f.text_field :password %>
  


 
  

  <%= f.submit "Ready!" %>
<% end %>

