<h1> You are in the new view for services </h1>

<%= form_for @service do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br/>
  <%= f.label :content  %>
  <%= f.text_field :content %><br/>
  <%= f.label :skills %>
  <%= f.text_field :skills %><br/>
  
  <%= f.submit "Ready!" %>
<% end %>