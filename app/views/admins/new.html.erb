<h1> You are in the new for admin</h1>


<%= form_for @admin do |f| %>
 <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br/>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>
  <%= f.label :email %>
  <%= f.text_field :email %><br/>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Ready!" %>
<% end %>