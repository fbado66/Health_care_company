

<div class="container">
    <h2>Welcome back Admin</h2>
    <br>
        <% if @error %>
            <p><%= @error %></p>
        <% end %>
        <%= form_tag "/send_the_form_here_3" do %>
            <strong><%= label_tag :first_name %></strong>
            <%= text_field_tag :first_name %><br>
            <strong><%= label_tag :password %></strong>
            <%= password_field_tag :password %><br>
            <%= submit_tag "Login!", :class => "btn btn-success" %>
        <% end %>
</div>